# Paths to ONNX Runtime - change these to your setup
ORT_INCLUDE = /home/gpu401/cv4_computer_vision_starter_code/final_project/ADAS-System/onnxruntime-linux-aarch64-1.21.0/include
ORT_LIB = /home/gpu401/cv4_computer_vision_starter_code/final_project/ADAS-System/onnxruntime-linux-aarch64-1.21.0/lib


# Compiler and flags
CXX = g++
CXXFLAGS = -std=c++17 -O2 `pkg-config --cflags opencv4` -I$(ORT_INCLUDE)
LDFLAGS = `pkg-config --libs opencv4` -L$(ORT_LIB) -lonnxruntime


simple_yolo: simple_yolo.cpp
	$(CXX) $(CXXFLAGS) $< -o $@ $(LDFLAGS)
	
